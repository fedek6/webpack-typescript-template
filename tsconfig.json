{
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "outDir": "./dist/",
    "lib": ["dom", "esnext"],
    "jsx": "react"
  },
  "include": ["src/scripts/**/*"]
}
